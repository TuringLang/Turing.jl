    <span class="k">end</span>
<span class="k">end</span><span class="x">;</span>

<span class="n">model</span> <span class="o">=</span> <span class="n">fitlv</span><span class="x">([</span><span class="n">averagedata</span><span class="x">,</span><span class="n">real_vars</span><span class="x">])</span>
